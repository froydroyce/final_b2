<h1><u>Courses</u></h1>

<% @courses.each do |course| %>
  <section id="course-<%= course.id %>">
    <h2><u><%= course.name %></u></h2>
    <p>Enrolled Students</p>
    <ul>
      <% course.students_by_grade.each do |student| %>
          <li><%= student.name %>, Grade: <%= course.grade_for(student) %></li>
      <% end %>
    </ul>
  </section>
<% end %>
